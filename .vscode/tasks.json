{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "shell",
      "label": "C/C++: clang++ build active file",
      "command": "/usr/bin/clang++",
      "args": [
        "-std=c++20",
        "-stdlib=libc++",
        "-g",
        "/Users/tizzymatic/Documents/GitHub/Data-Structures-and-Algorithm-Solutions/fully-written-solutions/leetcode/cpp/main.cpp",
        "/Users/tizzymatic/Documents/GitHub/Data-Structures-and-Algorithm-Solutions/fully-written-solutions/leetcode/cpp/two_sum.cpp",
        "/Users/tizzymatic/Documents/GitHub/Data-Structures-and-Algorithm-Solutions/fully-written-solutions/leetcode/cpp/linked_list_cycle.cpp",
        "/Users/tizzymatic/Documents/GitHub/Data-Structures-and-Algorithm-Solutions/fully-written-solutions/leetcode/cpp/remove_nth_from_end_of_list.cpp",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": ["$gcc"],
      "group": "build",
      "detail": "Task generated by Debugger."
    },
    {
      "label": "swift: Build Debug swift",
      "type": "shell",
      "command": "swift build",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$swiftc"],
      "options": {
        "cwd": "${workspaceFolder}/fully-written-solutions/practice/swift"
      }
    },
    {
      "label": "swift: Run Debug swift",
      "type": "shell",
      "command": "swift run",
      "group": {
        "kind": "test",
        "isDefault": true
      },
      "problemMatcher": ["$swiftc"],
      "options": {
        "cwd": "${workspaceFolder}/fully-written-solutions/practice/swift"
      }
    }
  ]
}